<mvc:View controllerName="salesplanningreport.controller.Home"
    displayBlock="true"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f">
     <f:DynamicPage  headerExpanded="true" headerPinned="true">
        <f:title >
            <f:DynamicPageTitle >
                <f:heading >
                
                    <Title class="bold tworem" textAlign="Center" text="{i18n>salesPlanningReport}"/>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <f:header >
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <HBox justifyContent="Center" alignItems="Center">
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>Pianificazionediacquisizioni}" press="navToAcquisizioni" url="https://www.sap.com/">
         
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>reportpianificazionevendite}" press="navToReportVendite" url="https://www.sap.com/">
 
                </GenericTile>

            </HBox>
        </f:content>
    </f:DynamicPage>
</mvc:View>
